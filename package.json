{
  "name": "awell-intake",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "format": "prettier --write --ignore-unknown **/*.{ts,tsx,js,jsx}",
    "preview": "vite preview",
    "prepare": "husky",
    "clean": "rimraf dist",
    "bots:build": "npm run clean && tsc && node src/bots/build-bots.mjs",
    "bots:deploy": "node src/bots/deploy-bots.mjs"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@medplum/bot-layer": "^4.0.3",
    "@medplum/fhirtypes": "4.0.3",
    "@types/moment": "^2.11.29",
    "@types/node": "20.17.22",
    "@types/react": "18.3.18",
    "@types/react-big-calendar": "^1.16.1",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react": "4.3.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "8.5.3",
    "postcss-preset-mantine": "1.17.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.26.1",
    "vite": "6.2.0"
  },
  "dependencies": {
    "@mantine/core": "^7.17.2",
    "@mantine/dates": "^7.17.2",
    "@mantine/form": "^7.17.2",
    "@mantine/hooks": "^7.17.2",
    "@mantine/notifications": "^7.17.2",
    "@medplum/core": "4.1.6",
    "@medplum/react": "4.0.3",
    "@tabler/icons-react": "3.17.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-big-calendar": "^1.18.0",
    "react-dom": "18.2.0",
    "react-router": "7.2.0",
    "react-router-dom": "^7.3.0"
  }
}
